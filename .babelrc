{
  "presets": [
    "@babel/preset-env",
    ["@babel/typescript", {
      "jsxPragma": "DOM"
    }]
  ],
  "plugins": [
    ["@babel/plugin-transform-react-jsx", {
      "pragma": "DOM.hydrate",
      "pragmaFrag": "DOM.FRAGMENT"
    }],
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose" : true }],
    "@babel/plugin-syntax-dynamic-import"
  ]
}
